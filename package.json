{
  "name": "admisi-it-ac-id",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply prod-admisi-it-ac-id --local",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview prod-admisi-it-ac-id-preview",
    "db:migrate:prod": "wrangler d1 migrations apply prod-admisi-it-ac-id --remote",
    "db:studio:local": "cross-env DB_LOCAL_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "cross-env NODE_ENV='preview' drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio",
    "build": "next build",
    "dev": "next dev",
    "pages:build": "npm next-on-pages",
    "pages:dev": "npm pages:build && npm wrangler pages dev",
    "pages:deploy": "npm pages:build && npm wrangler pages deploy"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.9.0",
    "@tanstack/react-query": "^5.25.0",
    "bootstrap": "^5.3.3",
    "next": "14.1.3",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.4",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.8",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.11.3",
    "@cloudflare/workers-types": "^4.20240512.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@types/better-sqlite3": "^7.6.9",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "better-sqlite3": "^9.4.3",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.21.4",
    "drizzle-orm": "^0.30.10",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "wrangler": "^3.36.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.30.0"
  }
}
